{
  "singleQuote": true,
  "trailingComma": "all",
  "printWidth": 100,
  "bracketSameLine": true,
  "overrides": [
    {
      "files": "*.html",
      "options": {
        "parser": "html"
      }
    },
    {
      "files": "*.scss",
      "options": {
        "parser": "css"
      }
    }
  ]
}
